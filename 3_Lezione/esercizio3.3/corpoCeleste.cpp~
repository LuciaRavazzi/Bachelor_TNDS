#include "corpoCeleste.h"
#include <iostream>

using namespace std;

corpoCeleste::corpoCeleste(string nome,double raggio,double massa):particella(massa,0){
	_nome=nome;
	_raggio=raggio;
}

	
void corpoCeleste::print() const{
	cout << "Il pianeta " << _nome << " di massa " << _massa << " e raggio" << _raggio << endl;
}

